    "apollo-boost": "^0.3.1",
    "babel-loader": "^8.0.5",
    "chartist": "^0.11.2",
    "element-ui": "^2.8.2",
    "vee-element": "^1.0.0-alpha.4",
    "video.js": "^7.5.4",
    "vue": "^2.6.10",
    "vue-apollo": "^3.0.0-beta.29",
    "vue-i18n": "^8.0.0",
    "vue-router": "^3.0.6",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "graphql-tag": "^2.9.0",
    "node-sass": "^4.13.1",
    "sass-loader": "^7.1.0",
    "vue-cli-plugin-apollo": "^0.20.0",
    "vue-cli-plugin-element": "^1.0.1",
